{
  "name": "patientoutcome-frontend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "regenerate-api": "rm -rf src/api && npx @openapitools/openapi-generator-cli generate -i http://localhost:40001/openapi/v1/swagger.json -g typescript-fetch -o src/api"
  },
  "dependencies": {
    "@date-io/dayjs": "^3.2.0",
    "@jsonforms/core": "3.7.0-alpha.3",
    "@jsonforms/vue": "3.7.0-alpha.3",
    "@jsonforms/vue-vuetify": "3.7.0-alpha.3",
    "@mdi/font": "^7.4.47",
    "@vuepic/vue-datepicker": "^11.0.2",
    "@vueuse/components": "^13.3.0",
    "@vueuse/core": "^13.3.0",
    "ajv": "^8.17.1",
    "animate.css": "^4.1.1",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "markdown-it": "^14.1.0",
    "pinia": "^3.0.1",
    "reveal.js": "^5.2.1",
    "vite-plugin-vuetify": "^2.1.1",
    "vue": "^3.5.22",
    "vue-chartjs": "^5.3.3",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.0",
    "vuetify": "^3.10.11"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.20.0",
    "@playwright/test": "^1.51.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "^1.1.36",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  }
}
